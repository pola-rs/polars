{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "outDir": "./bin",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@polars": [
        "polars",
      ],
      "@polars/*": [
        "polars/*"
      ]
    }
  },
  "exclude": [
    "./jest.config.ts"
  ]
}